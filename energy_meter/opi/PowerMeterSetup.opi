    #if ((apprun == 0) and (appstop == 0)) or ((apprun == 1) and (appstop == 0)) or ((apprun == 1) and (appstop == 1)):
    #    return
    
    #elif (apprun == 0 and appstop == 1):
     

    isInWorkspace = False
    isAppend = False


    filePath = display.getWidget("filePath").getPropertyValue("text");
    type_filePath = type(filePath)
    type_filePath_s = str(type_filePath)
    ConsoleUtil.writeInfo("type filePath = " + type_filePath_s)
    ConsoleUtil.writeInfo(filePath)
        
        
    directory = ntpath.dirname(filePath)
    filename = ntpath.basename(filePath)
    abspath = ntpath.abspath(filePath)
    backslash = '\\'
    directory_full = directory + backslash
        
    if filePath is None:
        return    
           
    os.chdir(directory_full)
                
    header = open("header.txt", "r+")
    
    header_str = header.read();
        
    ConsoleUtil.writeInfo(header_str)
        
    text = open(filename,"r+")
        
    text_str = text.read();
        
    text.truncate()
        
    text.close()
        
    final = open(filename,"r+")
       
    final.write(header_str)
        
    final.write(text_str)
        
    header.close()
        
    os.remove("header.txt")
        
    final.close()
        
        
def create_header():
        
    appstop = PVUtil.getLong(pvs[0])
    apprun = PVUtil.getLong(pvs[1])
        #if ((apprun == 0) and (appstop == 0)) or ((apprun == 1) and (appstop == 0)) or ((apprun == 1) and (appstop == 1)):
        #    return
        #
    if (apprun == 0 and appstop == 1):
        # 
        header_nr = PVUtil.getLong(pvs[6])
        
        if header_nr == 0:
               
                           
            isInWorkspace = False
                   
            isAppend = True

            filePath1 = display.getWidget("filePath").getPropertyValue("text");
                                
            directory = ntpath.dirname(filePath1)
                    
            filename = ntpath.basename(filePath1)
                    
            abspath = ntpath.abspath(filePath1)
                            
            backslash = '\\'
                    
            filename_header = 'header.txt'
                    
            filePath = directory + backslash + filename_header
                
            ConsoleUtil.writeInfo(filename_header)
            ConsoleUtil.writeInfo(directory)
            ConsoleUtil.writeInfo(filePath)

            if filePath1 is None:
                return
                    
                    
            fileContent_time = generate_file_content_time()
            fileContent1 = generate_file_content_time_stamp()       
            fileContent_space = generate_file_space()       
            fileContent0 = generate_file_content_pulse_number()
            fileContent2 = generate_new_line()
            fileContent_a_e_s = generate_file_content_a_e_s()
            fileContent_a_e = generate_file_content_a_e()
            fileContent_rms_s = generate_file_content_rms_s()
            fileContent_rms = generate_file_content_rms()
                    #fileContent_worst_rms_s = generate_file_content_worst_rms_s()
                    #fileContent_worst_rms = generate_file_content_worst_rms()


            FileUtil.writeTextFile(filePath, isInWorkspace, fileContent2, isAppend)
            FileUtil.writeTextFile(filePath, isInWorkspace, fileContent_time, isAppend)
            FileUtil.writeTextFile(filePath, isInWorkspace, fileContent1, isAppend)
            FileUtil.writeTextFile(filePath, isInWorkspace, fileContent2, isAppend)
            FileUtil.writeTextFile(filePath, isInWorkspace, fileContent_space, isAppend)
            FileUtil.writeTextFile(filePath, isInWorkspace, fileContent0, isAppend)
            FileUtil.writeTextFile(filePath, isInWorkspace, fileContent2, isAppend)
            FileUtil.writeTextFile(filePath, isInWorkspace, fileContent_a_e_s, isAppend)
            FileUtil.writeTextFile(filePath, isInWorkspace, fileContent_a_e, isAppend)
            FileUtil.writeTextFile(filePath, isInWorkspace, fileContent2, isAppend)
            FileUtil.writeTextFile(filePath, isInWorkspace, fileContent_rms_s, isAppend)
            FileUtil.writeTextFile(filePath, isInWorkspace, fileContent_rms, isAppend)
