#def absolute(pulse_new,pulse_old):
def absolute():

    pulse_old = PVUtil.getDouble(pvs[0])
    #get temporary value of pulse

    pulse_new = PVUtil.getDouble(pvs[1])
    #get new pulse value 
    
    pvs[0].setValue(pulse_new)
    #set "new old" value
    
    
    if pulse_old == 0:
        return
    
    stab = (abs(pulse_old-pulse_new)*100)/pulse_old
    #compute sts stability
    
    pvs[2].setValue(stab)
    #save value of stability
    
    
    return stab
        
def worst():

    
    worst_old = PVUtil.getDouble(pvs[3])
    #get old value 
    
    worst_new = PVUtil.getDouble(pvs[2])
    #get new worst value
    
    if worst_new > worst_old:
         #worst_old = worst_new
         pvs[3].setValue(worst_new)
         
    return worst_old
         
if __name__ == '__main__':


    absolute()
    worst()
    
    
    
    
    
           
