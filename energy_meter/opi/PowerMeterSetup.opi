        <scriptText><![CDATA[from org.csstudio.opibuilder.scriptUtil import PVUtil, FileUtil, ConsoleUtil
from org.eclipse.jface.dialogs import MessageDialog

import sys
import os
import ntpath
import math


#compute average energy from previous value 

def average_energy():

    aver_old = PVUtil.getDouble(pvs[0])
    #get old average energy value
    
    pulse_new = PVUtil.getDouble(pvs[1])
    #get new pulse value 
    
    counter = PVUtil.getLong(pvs[2])
    #actual counter value
    
    if counter == 0: 
        return 
    
    #pvs[0].setValue(aver_new)
    #set "new old" value
    
    
    #compute new average energy
    #average_energy = (average_energy_old*(count-1) + pulse)/(count);
    
    aver_new = (aver_old*(counter - 1) + pulse_new)/(counter)
    
    
    #stab = (abs(pulse_old-pulse_new)*100)/pulse_old
    #compute sts stability
    
    pvs[0].setValue(aver_new)
    #save value of new average energy
    
    pvs[4].setValue(aver_old)
    #save value of old average energy
    
    
    return aver_new
    
def standard_deviation():

        #double s_d(int count, double s_d_old, double pulse, double aver_old, double aver_cur)
        #     {
        #         double s_d;
        #	 
        #         s_d = sqrt(((count-1)*pow(s_d_old,2) + (pulse - aver_old)*(pulse - aver_cur))/(count));
        #         
        # return(s_d);  
        #    }

    counter = PVUtil.getLong(pvs[2])
    #actual counter value
    
    if counter == 0:
        return
    
    s_d_old = PVUtil.getDouble(pvs[3])
    # old standard deviation value 
    
    pulse_new = PVUtil.getDouble(pvs[1])
    #get new pulse value
    
    aver_old = PVUtil.getDouble(pvs[4])
    #get value of old average energy
    
    aver_new = PVUtil.getDouble(pvs[0])
    #get value of new average energy
    
    stan_dev = math.sqrt(((counter-1)*(s_d_old**2) + (pulse_new - aver_old)*(pulse_new - aver_new))/(counter))

    pvs[3].setValue(stan_dev)    
        
    return stan_dev       


def root_mean_square():

    #root_mean_square = (standard_dev/average_energy)*100;
    
    stan_dev = PVUtil.getDouble(pvs[3])
    #stan_dev = standard_deviation()
    aver_ener = average_energy()


    if stan_dev is not None and aver_ener is not None:

        rms = (stan_dev/aver_ener)*100
    
        pvs[5].setValue(rms)
    
        return rms
    
    
    
if __name__ == '__main__':
    #average_energy()
    standard_deviation()
    root_mean_square()]]></scriptText>
